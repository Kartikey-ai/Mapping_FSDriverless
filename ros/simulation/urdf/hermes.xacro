<?xml version="1.0" ?>
<robot name="hermes">
  <link name="base_link">
    <inertial>
      <mass value="1.0" />
      <inertia ixx="0.015" iyy="0.0375" izz="0.0375" ixy="0" ixz="0" iyz="0" />
    </inertial>
    <visual>
      <geometry>
        <box size="1.16195 0.651 0.77596" />
      </geometry>
      <material name="silver">
        <color rgba="0.75 0.75 0.75 1" />
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.6 0.3 0.3" />
      </geometry>
    </collision>
  </link>
  <link name="back_caster">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="0.00083" iyy="0.00083" izz="0.000167" ixy="0" ixz="0" iyz="0" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.1 0.1 0.77596" />
      </geometry>
      <material name="silver" />
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.77596" />
      </geometry>
    </collision>
  </link>
  <joint name="back_caster_joint" type="continuous">
    <axis xyz="0 0 1" />
    <parent link="base_link" />
    <child link="back_caster" />
    <origin rpy="0 0 0" xyz="-0.491695 0 0" />
  </joint>
  <link name="back_wheel">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="5.1458e-5" iyy="5.1458e-5" izz="6.125e-5" ixy="0" ixz="0" iyz="0" />
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
    </collision>

  </link>
  <joint name="back_wheel_joint" type="continuous">
    <axis xyz="0 0 1" />
    <parent link="back_caster" />
    <child link="back_wheel" />
    <origin rpy="-1.5708 0 0" xyz="0.05 0 -.38798" />
  </joint>
  <link name="front_caster">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="0.00083" iyy="0.00083" izz="0.000167" ixy="0" ixz="0" iyz="0" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.1 0.1 0.77596" />
      </geometry>
      <material name="silver" />
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.77596" />
      </geometry>
    </collision>
  </link>
  <joint name="front_caster_joint" type="continuous">
    <axis xyz="0 0 1" />
    <parent link="base_link" />
    <child link="front_caster" />
    <origin rpy="0 0 0" xyz="0.491695 0 0" />
  </joint>
  <link name="front_wheel">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="5.1458e-5" iyy="5.1458e-5" izz="6.125e-5" ixy="0" ixz="0" iyz="0" />
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
    </collision>

  </link>
  <joint name="front_wheel_joint" type="continuous">
    <axis xyz="0 0 1" />
    <parent link="front_caster" />
    <child link="front_wheel" />
    <origin rpy="-1.5708 0 0" xyz="0.05 0 -.38798" />
  </joint>
  <link name="right_wheel">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="5.1458e-5" iyy="5.1458e-5" izz="6.125e-5" ixy="0" ixz="0" iyz="0" />
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1" />
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
    </collision>
  </link>
  <joint name="right_wheel_joint" type="continuous">
    <axis xyz="0 0 1" />
    <parent link="base_link" />
    <child link="right_wheel" />
    <origin rpy="-1.5708 0 0" xyz="0.159795 -.3005 -.38798" />
  </joint>
  <link name="left_wheel">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="5.1458e-5" iyy="5.1458e-5" izz="6.125e-5" ixy="0" ixz="0" iyz="0" />
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
      <material name="black" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1" />
      </geometry>
    </collision>
  </link>
  <joint name="left_wheel_joint" type="continuous">
    <axis xyz="0 0 1" />
    <parent link="base_link" />
    <child link="left_wheel" />
    <origin rpy="-1.5708 0 0" xyz="0.159795 0.3005 -.38978" />
  </joint>
  <link name = "camera_center"/>
  <joint name = "camera_center_base_link" type = "fixed">
    <origin xyz="0.630975 0.0 -0.1807467" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="camera_center" />
  </joint>     
  <link name="camera_link_left">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>
  <joint name="camera_joint_left" type="fixed">
    <origin xyz="0.0 0.1 0.0" rpy="0 0 0" />
    <parent link="camera_center" />
    <child link="camera_link_left" />
  </joint>
    <link name="camera_link_right">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>
  <joint name="camera_joint_right" type="fixed">
    <origin xyz="0.0 -0.1 0.0" rpy="0 0 0" />
    <parent link="camera_center" />
    <child link="camera_link_right" />
  </joint>
  <link name="left_camera_optical_frame"/>  
  <joint name="left_camera_optical_frame_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0" />
    <parent link="camera_link_left" />
    <child link="left_camera_optical_frame"/>
  </joint>
  <gazebo>
    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <leftJoint>left_wheel_joint</leftJoint>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <publishWheelTF>false</publishWheelTF>
      <publishOdomTF>true</publishOdomTF>
      <wheelTorque>5</wheelTorque>
      <wheelAcceleration>0</wheelAcceleration>
      <updateRate>100</updateRate>
      <odometrySource>world</odometrySource> <!-- 'encoder' instead of 'world' is also possible -->
      <publishTf>1</publishTf>
      <rosDebugLevel>na</rosDebugLevel>
      <rightJoint>right_wheel_joint</rightJoint>
      <robotBaseFrame>base_link</robotBaseFrame>
      <wheelSeparation>0.25</wheelSeparation>
      <wheelDiameter>0.07</wheelDiameter>
      <publishWheelJointState>true</publishWheelJointState>
      <commandTopic>cmd_vel</commandTopic>
      <robotBaseFrame>base_link</robotBaseFrame>
    </plugin>
  </gazebo>
